{
  "name": "hopkins-resume",
  "version": "0.1.0",
  "bin": {
    "hopkins-resume": "bin/hopkins-resume.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "lint:js": "eslint 'site-contents/js/main.js'",
    "lint:html": "htmlhint 'site-contents/**/*.html'",
    "lint": "npm run lint:js && npm run lint:html",
    "format": "prettier --write 'site-contents/**/*.{js,html,css}'",
    "format:check": "prettier --check 'site-contents/**/*.{js,html,css}'",
    "ci": "npm run lint && npm run format:check && npm test"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/node": "10.17.27",
    "aws-cdk": "2.19.0",
    "eslint": "^8.57.0",
    "htmlhint": "^1.1.4",
    "jest": "^26.6.3",
    "prettier": "^3.2.5",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.0.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "aws-cdk-lib": "2.19.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.16"
  }
}
